        this.info = info;
    }
    
    public T getInfo() {
        return info;
    }
    
    public void setInicio(NoListaDupla<T> proximo) {
        this.inicio = proximo;  // Corrigido: atribui ao pr贸ximo
    }
    
    public NoListaDupla<T> getInicio() {
        return inicio;
    }
    
    public void setFim(NoListaDupla<T> anterior) {
        this.fim = anterior;    // Corrigido: atribui ao anterior
    }
    
    public NoListaDupla<T> getFim() {
